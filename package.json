{
  "name": "sphinx-v2",
  "version": "1.0.0",
  "description": "A React/Relay frontend and GraphQL backend for futbol24.com",
  "main": "schema.js",
  "scripts": {
    "build": "npm run relay && webpack",
    "lint": "standard",
    "relay": "node scripts/generate-schema.js && relay-compiler --src ./src/web-ui --schema .schema.graphql",
    "devserver": "webpack-dev-server --config webpack.dev.js",
    "start": "node src/server.js",
    "postinstall": "npm run build && npm prune --production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Petros Angelatos <petrosagg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/petrosagg/sphinx.git"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "bluebird": "^3.5.1",
    "compression": "^1.7.1",
    "connect-history-api-fallback": "^1.4.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.3",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.19",
    "memoizee": "^0.4.11",
    "react": "^16.0.0",
    "react-csv": "^1.0.14",
    "react-dom": "^16.0.0",
    "react-relay": "^1.4.1",
    "relay-runtime": "^1.4.1",
    "request": "^2.83.0",
    "universal-router": "^5.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "html-webpack-plugin": "^2.30.1",
    "relay-compiler": "^1.4.1",
    "standard": "^10.0.3",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "engines": {
    "node": "^v8.8.1",
    "npm": "^5.5.1"
  }
}
